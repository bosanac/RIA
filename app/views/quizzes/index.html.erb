<p id="notice"><%= notice %></p>

<h1>Listing Quizzes</h1>

<table>
  <thead>
    <tr>
      <th>Naslov</th>
      <th>Opis</th>
      <th>Datumstart</th>
      <th>Datumstop</th>
      <th>Pokusaja</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quizzes.each do |quiz| %>
      <tr>
        <td><%= quiz.naslov %></td>
        <td><%= quiz.opis %></td>
        <td><%= quiz.datumstart %></td>
        <td><%= quiz.datumstop %></td>
        <td><%= quiz.pokusaja %></td>
        <td><%= link_to 'Show', quiz %></td>
        <td><%= link_to 'Edit', edit_quiz_path(quiz) %></td>
        <td><%= link_to 'Destroy', quiz, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quiz', new_quiz_path %>
